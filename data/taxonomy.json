  "topics": [
    {
      "id": "u1_t1_intro-bas-hc",
      "label": "Getting Started With Bas For Hana Cloud",
      "description": "Overview of the unit outcomes and BAS’s role in SAP HANA Cloud modeling."
    },
    {
      "id": "u1_t2_dev-spaces",
      "label": "Development Spaces And Extensions",
      "description": "Create and manage BAS development spaces and select required extensions like the Calculation View Editor."
    },
    {
      "id": "u1_t3_import-git",
      "label": "Importing Projects And Using Git",
      "description": "Import existing projects into the workspace and use Git for sharing and collaboration."
    },
    {
      "id": "u1_t4_unit-quiz",
      "label": "Unit Knowledge Check",
      "description": "Quick quiz reinforcing project import and collaboration choices."
    },
    {
      "id": "u2_t1_concepts",
      "label": "Modeling Terminology And Key Concepts",
      "description": "Define measures and attributes and foundational modeling terms in SAP HANA Cloud."
    },
    {
      "id": "u2_t2_dimension-views",
      "label": "Creating Dimension Calculation Views",
      "description": "Build reusable master-data views with attributes, filters, and hierarchies."
    },
    {
      "id": "u2_t3_cube-views",
      "label": "Creating Cube Calculation Views",
      "description": "Create measure-based cubes with automatic aggregation by requested attributes."
    },
    {
      "id": "u2_t4_time-dimensions",
      "label": "Time Based Dimension Views",
      "description": "Generate date-derived attributes from a base date using time dimensions."
    },
    {
      "id": "u3_t1_data-preview",
      "label": "Checking Calculation View Output",
      "description": "Use Data Preview in BAS to validate calculation view results."
    },
    {
      "id": "u3_t2_common-features",
      "label": "Common Features And Column Semantics",
      "description": "Configure the Semantics node, assign column types, and define behavior for consumers."
    },
    {
      "id": "u3_t3_top-node",
      "label": "Understanding The Top View (Semantics) Node",
      "description": "Explain the mandatory Semantics top node and its enrichment capabilities."
    }
